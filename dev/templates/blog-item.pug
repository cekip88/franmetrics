extends template/main
append vars
	-
		page_title = 'Blog'
block content
	section.blog
		.section
			.row.confidence-row
				h2.confidence-title.advantages-title.faded
					| Fran Metrics#[br]
					| #[span.green Blog]
				h3.confidence-subtitle.advantages-subtitle.faded
					| For Franchisors#[br]
					| & Franchisees
			a.blog-inner_back
				svg
					use(xlink:href="/img/sprite.svg#back")
				span
					| Back
			.blog-inner
				.blog-inner_img
					+img('blog.jpg')
				.blog-inner_date
					| 03.10.2021
				h2.blog-inner_title
					| Franchise Data Problems Are Really Culture Problems
